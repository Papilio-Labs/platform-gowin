{
  "build": {
    "arduino": {
      "ldscript": "esp32s3_out.ld",
      "memory_type": "qio_qspi",
      "partitions": "default.csv"
    },
    "core": "esp32",
    "extra_flags": [
      "-DARDUINO_PAPILIO_RETROCADE_ESP32S3",
      "-DARDUINO_USB_MODE=1",
      "-DARDUINO_USB_CDC_ON_BOOT=1",
      "-DBOARD_HAS_PSRAM",
      "-DBOARD_HAS_FPGA",
      "-DFPGA_ENABLED=1",
      "-DESP32_S3=1",
      "-DFPGA_SPI_CLK=12",
      "-DFPGA_SPI_MISO=9",
      "-DFPGA_SPI_MOSI=11",
      "-DFPGA_SPI_CS=10",
      "-DFPGA_RESET_PIN=26"
    ],
    "f_cpu": "240000000L",
    "f_flash": "80000000L",
    "flash_mode": "qio",
    "psram_type": "qio",
    "mcu": "esp32s3",
    "variant": "esp32s3",
    "device": "GW2A-18",
    "fpga_family": "GW2A",
    "fpga_package": "PG256",
    "fpga_speed": "C8/I7",
    "fpga_project": "fpga/project.gprj",
    "fpga_top_module": "top",
    "fpga_device_full": "GW2A-LV18PG256C8/I7",
    "gowin_path": "C:/Gowin/Gowin_V1.9.11.03_Education_x64"
  },
  "connectivity": [
    "wifi",
    "bluetooth",
    "ble"
  ],
  "debug": {
    "openocd_target": "esp32s3.cfg"
  },
  "frameworks": [
    "arduino",
    "espidf"
  ],
  "name": "Papilio RetroCade ESP32-S3+FPGA",
  "upload": {
    "flash_size": "4MB",
    "maximum_ram_size": 524288,
    "maximum_size": 4194304,
    "require_upload_port": true,
    "speed": 921600,
    "protocol": "esptool",
    "use_1200bps_touch": true,
    "wait_for_upload_port": true
  },
  "url": "https://github.com/Papilio-Labs/papilio_retrocade_hardware",
  "vendor": "Papilio"
}
